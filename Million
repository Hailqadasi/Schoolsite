// ุฎูุท ุงูุฃุณุฆูุฉ ุนุดูุงุฆููุง
questions = questions.sort(() => Math.random() - 0.5);

function showQuestion() {
  if (currentQuestion >= questions.length) {
    document.getElementById('question').innerHTML = "ููุฏ ุฃูููุช ุฌููุน ุงูุฃุณุฆูุฉ!";
    document.getElementById('options').innerHTML = "";
    document.getElementById('score').innerHTML = `ุงูุฌุงุฆุฒุฉ ุงูููุงุฆูุฉ: ${prize} ุฑูุงู`;
    return;
  }
  
  const q = questions[currentQuestion];
  document.getElementById('question').innerHTML = q.question;
  
  let optionsHTML = "";
  q.options.forEach(option => {
    optionsHTML += `<button onclick="checkAnswer('${option}')">${option}</button>`;
  });
  document.getElementById('options').innerHTML = optionsHTML;
  
  // ุชุญุฏูุซ ุญุงูุฉ ุฃุฒุฑุงุฑ ุงููุณุงุนุฏุฉ
  document.getElementById('fifty').disabled = !lifelines.fifty;
  document.getElementById('audience').disabled = !lifelines.audience;
  document.getElementById('skip').disabled = !lifelines.skip;
  
  if (!lifelines.fifty) document.getElementById('fifty').classList.add('disabled');
  else document.getElementById('fifty').classList.remove('disabled');
  
  if (!lifelines.audience) document.getElementById('audience').classList.add('disabled');
  else document.getElementById('audience').classList.remove('disabled');
  
  if (!lifelines.skip) document.getElementById('skip').classList.add('disabled');
  else document.getElementById('skip').classList.remove('disabled');
}

function checkAnswer(selected) {
  const q = questions[currentQuestion];
  const buttons = document.querySelectorAll('.options button');
  let correctIndex = -1;
  
  buttons.forEach((btn, index) => {
    if (btn.textContent === q.answer) {
      correctIndex = index;
    }
    btn.disabled = true;
  });
  
  if (selected === q.answer) {
    document.querySelectorAll('.options button')[correctIndex].classList.add('correct');
    prize += 1000;
    document.getElementById('score').innerHTML = `ุงูุฌุงุฆุฒุฉ: ${prize} ุฑูุงู`;
    setTimeout(() => {
      currentQuestion++;
      showQuestion();
    }, 1500);
  } else {
    document.querySelectorAll('.options button')[correctIndex].classList.add('correct');
    buttons.forEach((btn, index) => {
      if (btn.textContent === selected) {
        btn.classList.add('wrong');
      }
    });
    setTimeout(() => {
      document.getElementById('question').innerHTML = "ุงูุชูุช ุงููุนุจุฉ!";
      document.getElementById('options').innerHTML = "";
      document.getElementById('score').innerHTML = `ุงูุฌุงุฆุฒุฉ ุงูููุงุฆูุฉ: ${prize} ุฑูุงู`;
    }, 1500);
  }
}

function useFifty() {
  if (!lifelines.fifty) return;
  
  const q = questions[currentQuestion];
  const buttons = document.querySelectorAll('.options button');
  const wrongOptions = [];
  
  buttons.forEach((btn, index) => {
    if (btn.textContent !== q.answer) {
      wrongOptions.push(index);
    }
  });
  
  // ุญุฐู ุฎูุงุฑูู ุนุดูุงุฆููู ุฎุงุทุฆูู
  for (let i = 0; i < 2; i++) {
    const randomIndex = Math.floor(Math.random() * wrongOptions.length);
    const btnIndex = wrongOptions[randomIndex];
    buttons[btnIndex].style.display = 'none';
    wrongOptions.splice(randomIndex, 1);
  }
  
  lifelines.fifty = false;
  document.getElementById('fifty').classList.add('disabled');
}

function useAudience() {
  if (!lifelines.audience) return;
  
  alert("ุงุณุชุดุงุฑุฉ ุงูุฌูููุฑ: ุงูุฌูููุฑ ููุถู ุงูุฎูุงุฑ ุงูุฃูู ุจูุณุจุฉ 60%");
  lifelines.audience = false;
  document.getElementById('audience').classList.add('disabled');
}

function useSkip() {
  if (!lifelines.skip) return;
  
  currentQuestion++;
  showQuestion();
  lifelines.skip = false;
  document.getElementById('skip').classList.add('disabled');
}

// ุฒุฑ ูุณุญ ุงูุฃุณุฆูุฉ (ูููุดุฑู)
document.getElementById('clear-questions-btn').addEventListener('click', function() {
  if (confirm("ูู ุฃูุช ูุชุฃูุฏ ูู ูุณุญ ุฌููุน ุงูุฃุณุฆูุฉุ")) {
    localStorage.removeItem('questions');
    alert("ุชู ูุณุญ ุฌููุน ุงูุฃุณุฆูุฉ");
  }
});

// ุจุฏุก ุงููุนุจุฉ
showQuestion();
</script>
</body>
</html>
