questions.sort(() => Math.random() - 0.5);

function startGame(){
  currentQuestion = 0;
  prize = 0;
  lastCheckpoint = 0;
  document.getElementById("card").innerHTML = `
    <div id="timer">โฑ๏ธ ุงูููุช ุงููุชุจูู: 20 ุซุงููุฉ</div>
    <div class="question" id="question"></div>
    <div class="options" id="options"></div>
    <div class="score" id="score">ุงูุฌุงุฆุฒุฉ ุงูุญุงููุฉ: 0 ุฑูุงู</div>
  `;
  showQuestion();
}

function startTimer(){
  clearInterval(timerInterval);
  questionTime = 20;
  timerInterval = setInterval(() => {
    document.getElementById("timer").innerText = `โฑ๏ธ ุงูููุช ุงููุชุจูู: ${questionTime} ุซุงููุฉ`;
    if(questionTime <= 0){
      clearInterval(timerInterval);
      endGame("โฐ ุงูุชูู ุงูููุช!");
    }
    questionTime--;
  }, 1000);
}

function showQuestion(){
  if(currentQuestion >= questions.length){
    showWinner();
    return;
  }
  
  document.getElementById("question").innerHTML = 
    `<span class="checkpoint">ุงูุณุคุงู ${currentQuestion + 1}/${questions.length}</span><br><br>${questions[currentQuestion].question}`;
  
  let optionsDiv = document.getElementById("options");
  optionsDiv.innerHTML = "";
  
  // ุฎูุท ุงูุฎูุงุฑุงุช ุนุดูุงุฆูุง
  let shuffledOptions = [...questions[currentQuestion].options];
  shuffledOptions.sort(() => Math.random() - 0.5);
  
  shuffledOptions.forEach(opt => {
    let button = document.createElement("button");
    button.innerText = opt;
    button.onclick = () => checkAnswer(opt);
    optionsDiv.appendChild(button);
  });
  
  document.getElementById("score").innerHTML = `ุงูุฌุงุฆุฒุฉ ุงูุญุงููุฉ: <span style="color:#ffd700">${prize} ุฑูุงู</span>`;
  
  // ุฅุธูุงุฑ ููุทุฉ ุงูุชุญูู ุฅุฐุง ูุตู ุฅูููุง
  if(checkpoints.includes(currentQuestion)) {
    lastCheckpoint = prize;
    let checkpointMessage = "";
    switch(currentQuestion) {
      case 19: // ุจุนุฏ ุงูุณุคุงู 20
        checkpointMessage = "๐ ูุตูุช ุฅูู ููุทุฉ ุงูุชุญูู ุงูุฃููู! ุงูุฌุงุฆุฒุฉ ุงููุถูููุฉ: 400 ุฑูุงู";
        break;
      case 29: // ุจุนุฏ ุงูุณุคุงู 30
        checkpointMessage = "๐ ูุตูุช ุฅูู ููุทุฉ ุงูุชุญูู ุงูุซุงููุฉ! ุงูุฌุงุฆุฒุฉ ุงููุถูููุฉ: 600 ุฑูุงู";
        break;
      case 49: // ุจุนุฏ ุงูุณุคุงู 50
        checkpointMessage = "๐ ูุตูุช ุฅูู ููุทุฉ ุงูุชุญูู ุงูุซุงูุซุฉ! ุงูุฌุงุฆุฒุฉ ุงููุถูููุฉ: 1000 ุฑูุงู";
        break;
    }
    document.getElementById("score").innerHTML += `<br><div class="checkpoint">โ ${checkpointMessage}</div>`;
  }
  
  startTimer();
}

function checkAnswer(selected){
  clearInterval(timerInterval);
  let correct = questions[currentQuestion].answer;
  let buttons = document.querySelectorAll("#options button");
  
  buttons.forEach(button => {
    if(button.innerText === correct) {
      button.classList.add("correct");
    }
    if(button.innerText === selected && selected !== correct) {
      button.classList.add("wrong");
    }
    button.disabled = true;
  });

  if(selected === correct){
    prize += 20;
    currentQuestion++;
    setTimeout(showQuestion, 1500);
  } else {
    setTimeout(() => endGame("โ ุฅุฌุงุจุฉ ุฎุงุทุฆุฉ!"), 1500);
  }
}

function showWinner(){
  clearInterval(timerInterval);
  let finalMessage = "";
  if (prize >= 1000) {
    finalMessage = "๐ ูุจุฑูู! ูุฒุช ุจุงูุฌุงุฆุฒุฉ ุงููุจุฑู!";
  } else if (prize >= 600) {
    finalMessage = "๐ ูุจุฑูู! ุญููุช ูุชูุฌุฉ ููุชุงุฒุฉ!";
  } else if (prize >= 400) {
    finalMessage = "๐ ุฃุญุณูุช! ุญููุช ูุชูุฌุฉ ุฌูุฏุฉ!";
  }
  
  document.getElementById("card").innerHTML = `
    <h1>๐ ูุฒุช ุจุงููุนุจุฉ!</h1>
    <p>${finalMessage}</p>
    <p>ุฃุฌุจุช ุนูู ุฌููุน ุงูุฃุณุฆูุฉ ุงูุจุงูุบุฉ ${questions.length} ุณุคุงูุงู ุจุดูู ุตุญูุญ</p>
    <p class="score">ุงูุฌุงุฆุฒุฉ ุงูููุงุฆูุฉ: ${prize} ุฑูุงู</p>
    <button class="start-btn" onclick="startGame()">ุฅุนุงุฏุฉ ุงููุนุจ</button>
  `;
}

function endGame(msg){
  clearInterval(timerInterval);
  let checkpointReached = "";
  if (lastCheckpoint >= 1000) {
    checkpointReached = "ูุตูุช ุฅูู ุฃุนูู ููุทุฉ ุชุญูู (1000 ุฑูุงู)";
  } else if (lastCheckpoint >= 600) {
    checkpointReached = "ูุตูุช ุฅูู ููุทุฉ ุงูุชุญูู ุงูุซุงููุฉ (600 ุฑูุงู)";
  } else if (lastCheckpoint >= 400) {
    checkpointReached = "ูุตูุช ุฅูู ููุทุฉ ุงูุชุญูู ุงูุฃููู (400 ุฑูุงู)";
  } else {
    checkpointReached = "ูู ุชุตู ุฅูู ุฃู ููุทุฉ ุชุญูู ุจุนุฏ";
  }
  
  document.getElementById("card").innerHTML = `
    <h2 style="color:#dc3545">${msg}</h2>
    <p>ุฃุฌุจุช ุนูู ${currentQuestion} ูู ${questions.length} ุณุคุงูุงู</p>
    <p>${checkpointReached}</p>
    <p class="score">ุงูุฌุงุฆุฒุฉ ุงูุชู ุญุตูุช ุนูููุง: ${lastCheckpoint} ุฑูุงู</p>
    <button class="start-btn" onclick="startGame()">ุฅุนุงุฏุฉ ุงููุนุจ</button>
  `;
}
